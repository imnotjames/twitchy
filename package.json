{
  "name": "twitchy",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "angular": "^1.3.1",
    "angular-animate": "^1.3.1",
    "angular-route": "^1.3.1",
    "bootstrap-sass": "^3.3.0",
    "browserify": "^6.2.0",
    "browserify-shim": "^3.8.0",
    "del": "^0.1.3",
    "gulp": "^3.8.9",
    "gulp-browserify": "^0.5.0",
    "gulp-concat": "^2.4.1",
    "gulp-jshint": "^1.8.6",
    "gulp-plumber": "^0.6.6",
    "gulp-sass": "^1.2.2",
    "gulp-sourcemaps": "^1.2.7",
    "gulp-uglify": "^1.0.1",
    "gulp-webserver": "^0.8.4",
    "require-dir": "^0.1.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "angular-route": "./node_modules/angular-route/angular-route.js",
    "angular-animate": "./node_modules/angular-animate/angular-animate.js"
  },
  "browserify-shim": {
    "angular": "angular",
    "angular-route": {
      "exports": "angular.module('ngRoute')",
      "depends": [
        "angular"
      ]
    },
    "angular-animate": {
      "exports": "angular.module('ngAnimate')",
      "depends": [
        "angular"
      ]
    }
  }
}
